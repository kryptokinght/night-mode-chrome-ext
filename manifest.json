{
    "manifest_version" : 2,
    "name" : "Night mode",
    "version" : "2.0",
    "content_scripts" : [
      {
        "matches" : [
          "<all_urls>"
        ],
        "js" : ["content.js"]
      }
    ],
    "background" : {
      "scripts" : ["background.js"]
    },
    "browser_action" : {
      "default_icon" : "kappa.png"
    }
}
